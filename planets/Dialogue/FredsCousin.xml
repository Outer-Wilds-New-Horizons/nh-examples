<DialogueTree xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/xen-42/outer-wilds-new-horizons/main/NewHorizons/Schemas/dialogue_schema.xsd">
	<NameField>Fred's Cousin</NameField>

	<DialogueNode>
		<Name>START</Name>
		<EntryCondition>DEFAULT</EntryCondition>
		<Dialogue>
			<Page>Oh wow, I didn't expect anyone to hear my distress call.</Page>
		</Dialogue>
		
		<DialogueOptionsList>
			<DialogueOption>
				<Text>How do you know my language?</Text>
				<DialogueTarget>1</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>How long have you been here?</Text>
				<DialogueTarget>2</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<RequiredLogCondition>EXAMPLES_FRED</RequiredLogCondition>
				<Text>Oh hey I met your cousin Fred on Timber Hearth!</Text>
				<DialogueTarget>3</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>
	
	<DialogueNode>
		<Name>1</Name>
		<Dialogue>
			<Page>Lazy writing.</Page>
		</Dialogue>
	</DialogueNode>

	<DialogueNode>
		<Name>2</Name>
		<Dialogue>
			<Page>At least a quarter million years. I ate my own children for sustenance. Their souls have sustained me thus far.</Page>
		</Dialogue>
	</DialogueNode>

	<DialogueNode>
		<Name>3</Name>
		<Dialogue>
			<Page>Who the hell is Fred?</Page>
		</Dialogue>
	</DialogueNode>

</DialogueTree>